{
	"version": 1,
	"include": ["/*"],
	"exclude": [],
	"routes": [
		{
			"src": "/(.*)/(__data\\.json)",
			"dest": "/$1/$2",
			"status": 200,
			"headers": {
				"Cache-Control": "public, max-age=0, must-revalidate"
			},
			"continue": true
		},
		{
			"src": "/(.*)",
			"dest": "/$1.html",
			"status": 200,
			"headers": {
				"Cache-Control": "public, max-age=0, must-revalidate"
			},
			"continue": true
		},
		{
			"src": "/(.*)",
			"dest": "/$1",
			"status": 200,
			"headers": {
				"Cache-Control": "public, max-age=0, must-revalidate"
			},
			"continue": true
		},
		{
			"src": "/(.*)",
			"dest": "/index.html",
			"status": 200,
			"headers": {
				"Cache-Control": "public, max-age=0, must-revalidate"
			}
		}
	]
}
